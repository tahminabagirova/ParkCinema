                                    <label class="form-check-label all" for="alllang">
                                        <p>ALL</p>
                                    </label>
                                </div>
                            </div>
                            <div class="sessionBar desk-version">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="movie-date" id="azerbaijan"
                                        value="4">
                                    <label class="form-check-label" for="azerbaijan">
                                        <p>AZERBAIJAN</p>
                                    </label>
                                </div>

                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="movie-date" id="russian"
                                        value="5">
                                    <label class="form-check-label" for="russian">
                                        <p>RUSSIAN</p>
                                    </label>
                                </div>

                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="movie-date" id="english"
