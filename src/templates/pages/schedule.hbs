                {{!-- date --}}
                <div class="date">
                    <div id="dateSchedule" class="dateInner">
                        <div class="owl-carousel owl-theme">
                            <div class="form-check">
                                <input class="form-check-input" type="radio" name="movie-date" id="d1" value="1"
